package ua.karavayev.beans;

import java.util.Date;

public class Route {
	private int id;
	private Date creationDate;
	private Place fromPlace;
	private Place toPlace;
	private RouteStatus routeStatus;
	private Vehicle vehicle;
	private Person driver;
	private Request request;
	
	public Route() {
		super();
	}
	
	
	
	public Route(Date creationDate, Place fromPlace, Place toPlace, RouteStatus routeStatus, Vehicle vehicle,
			Person driver, Request request) {
		super();
		this.creationDate = creationDate;
		this.fromPlace = fromPlace;
		this.toPlace = toPlace;
		this.routeStatus = routeStatus;
		this.vehicle = vehicle;
		this.driver = driver;
		this.request = request;
	}



	public Route(int id, Date creationDate, Place fromPlace, Place toPlace, RouteStatus routeStatus, Vehicle vehile,
			Person driver, Request request) {
		super();
		this.id = id;
		this.creationDate = creationDate;
		this.fromPlace = fromPlace;
		this.toPlace = toPlace;
		this.routeStatus = routeStatus;
		this.vehicle = vehile;
		this.driver = driver;
		this.request = request;
	}



	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public Date getCreationDate() {
		return creationDate;
	}
	public void setCreationDate(Date creationDate) {
		this.creationDate = creationDate;
	}
	public Place getFromPlace() {
		return fromPlace;
	}
	public void setFromPlace(Place fromPlace) {
		this.fromPlace = fromPlace;
	}
	public Place getToPlace() {
		return toPlace;
	}
	public void setToPlace(Place toPlace) {
		this.toPlace = toPlace;
	}
	public RouteStatus getRouteStatus() {
		return routeStatus;
	}
	public void setRouteStatus(RouteStatus routeStatus) {
		this.routeStatus = routeStatus;
	}
	public Vehicle getVehicle() {
		return vehicle;
	}
	public void setVehicle(Vehicle vehile) {
		this.vehicle = vehile;
	}
	public Person getDriver() {
		return driver;
	}
	public void setDriver(Person driver) {
		this.driver = driver;
	}
	public Request getRequest() {
		return request;
	}
	public void setRequest(Request request) {
		this.request = request;
	}
	
	
	
	
	
}
